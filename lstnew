#include "libft.h"

t_list	*ft_lstnew(void *content)
{
	t_list	*newlist;

	newlist = malloc(sizeof(t_list));
	if (!newlist)
		return (0);
	if (!content)
		newlist->content = 0;
	else
	{
		newlist->content = malloc(sizeof(content));
		if (!newlist->content)
			return (0);
	newlist->content = (dupliquer content);
	}
	newlist->next = 0;
	return (newlist);
}
